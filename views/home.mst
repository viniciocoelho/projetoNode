{{> header}}

<h1>Pagina Home: {{teste}}</h1>

{{#posts}}
    <div class="post">
        {{#photo}}
        <img src="/media/{{photo}}" />
        {{/photo}}
        <h3><a href="/post/{{slug}}">{{title}}</a></h3>
        <p>{{body}}</p>
        <a href="/post/{{slug}}/edit">[ editar ]</a>
    </div>
{{/posts}}


